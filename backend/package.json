{
  "name": "asatan-backend",
  "version": "1.0.0",
  "description": "朝たん計算アプリのバックエンドAPI",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "db:setup": "bun run src/scripts/setupDatabase.ts",
